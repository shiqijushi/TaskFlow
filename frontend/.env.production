# 生产环境配置
VITE_APP_TITLE=TaskFlow
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=TaskFlow - 高效的任务管理应用

# API配置
VITE_API_BASE_URL=https://api.taskflow.com/api
VITE_API_TIMEOUT=30000

# 认证配置
VITE_JWT_SECRET=production-secret-key
VITE_JWT_EXPIRES_IN=24h

# 存储配置
VITE_STORAGE_KEY=taskflow-storage
VITE_AUTH_STORAGE_KEY=taskflow-auth

# 生产模式配置
VITE_DEV_MODE=false
VITE_DEBUG=false
VITE_MOCK_API=false

# 主题配置
VITE_DEFAULT_THEME=light
VITE_THEME_STORAGE_KEY=taskflow-theme

# 功能开关
VITE_ENABLE_PWA=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true