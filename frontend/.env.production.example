# TaskFlow 前端生产环境配置

# API 配置
VITE_API_BASE_URL=https://api.taskflow.com/api
VITE_API_TIMEOUT=10000

# 应用配置
VITE_APP_NAME=TaskFlow
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=高效的任务和项目管理平台

# 功能开关
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_DARK_MODE=true

# 文件上传配置
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/*,application/pdf,text/*

# WebSocket 配置
VITE_WS_URL=wss://api.taskflow.com

# 地图服务配置（如果需要）
VITE_MAP_API_KEY=your-production-map-api-key

# 第三方服务配置
VITE_GOOGLE_CLIENT_ID=your-production-google-client-id
VITE_GITHUB_CLIENT_ID=your-production-github-client-id